     {{ product.name }}\n                </v-card-title>\n                <v-text-field\n                    v-model=\"quantity\"\n                    hide-details\n                    label=\"Anzahl\"\n                    class=\"pa-4\"\n                    type=\"number\"\n                />\n                <v-card-subtitle class=\"pt-0\">\n                    {{ item.quantity }} &#10005; {{ product.price }} € = {{ total }}\n                </v-card-subtitle>\n                <v-btn\n                    color=\"error\"\n                    outlined\n                    small\n                    class=\"mx-4\"\n                >\n                    <v-icon\n                        small\n                        left\n                    >\n                        fa-minus\n                    </v-icon>\n                    Entfernen\n                </v-btn>\n            </div>\n        </div>\n    </v-card>\n</template>\n\n<script>\n    export default {\n        name: \"HorizontalCard\",\n        props: {\n            item: Object,\n            updateCart: Function,\n            btnAction: String,\n            i: Number\n        },\n        data() {\n            return {\n                numberValue: 1\n            }\n        },\n        computed: {\n            product() {\n                return this.$store.state.products[this.item.itemId]\n            },\n            //productName() {\n                //return this.item.quantity > 1 ? this.product.name + \" (Anzahl: \" + this.item.quantity +\")\" : this.product.name\n            //},\n            total() {\n                return (this.$store.state.cart[this.i].quantity * this.product.price).toFixed(2) + \"€\"\n            },\n            quantity: {\n                set(value) {\n                    this.$store.commit('updateCart', { id: this.i, quantity: value })\n                },\n                get() {\n                    if(this.$store.state.cart[this.i]) {\n                        return this.$store.state.cart[this.i].quantity\n                    } else {\n                        /*this.$destroy();\n                        this.$el.parentNode.removeChild(this.$el);*/\n                        return 0\n                    }\n                }\n            }\n        }\n    }\n</script>\n\n<style>\n\n</style>"]}]}