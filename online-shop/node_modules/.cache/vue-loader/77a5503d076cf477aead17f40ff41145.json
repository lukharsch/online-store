nents: {\n            HorizontalCard\n        },\n        methods: {\n            removeItemFromCart(index) {\n                this.$store.state.cart.splice(index, 1)\n                var message = \"Element wurde vom Warenkorb entfernt\"\n                this.$store.commit('updateSnackbar', { message: message, variant: \"error\", show: true })\n            }\n        }\n    }\n</script>\n"]}]}